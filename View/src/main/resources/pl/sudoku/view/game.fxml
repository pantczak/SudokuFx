<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import java.net.URL?>
<VBox alignment="CENTER" minHeight="500.0" minWidth="330.0" spacing="20.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <HBox alignment="CENTER" prefHeight="71.0" spacing="20.0">
            <children>
                <Button fx:id="cancelButton" alignment="CENTER" mnemonicParsing="false" minWidth="120.0" text="%CancelButton" textAlignment="CENTER" />
                <Button fx:id="fileSaveButton" alignment="CENTER" mnemonicParsing="false" minWidth="120.0" text="%FileSaveButton" textAlignment="CENTER" />
                <VBox alignment="CENTER" prefWidth="160.0">
                    <TextField fx:id="dbSaveTextField" promptText="%DbSaveTextFieldPromptText" prefWidth="Infinity" alignment="CENTER"/>
                    <Button disable="true" fx:id="dbSaveButton" alignment="CENTER" mnemonicParsing="false" prefWidth="Infinity" text="%DbSaveButton" textAlignment="CENTER" />
                </VBox>
            </children>
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </padding>
        </HBox>
        <GridPane fx:id="sudokuGrid" alignment="CENTER" gridLinesVisible="true" />
    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
    <stylesheets>
        <URL value="@menu.css" />
    </stylesheets>
</VBox>
